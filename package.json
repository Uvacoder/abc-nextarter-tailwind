    "react-icons": "^4.4.0",
    "theme-change": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.4",
    "@commitlint/cz-commitlint": "^16.3.0",
    "@tailwindcss/typography": "^0.5.4",
    "@types/react": "17.0.39",
    "@types/tailwindcss": "^3.1.0",
